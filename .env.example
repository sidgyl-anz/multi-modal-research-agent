# Gemini API Configuration
GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# GCS Configuration
GCS_BUCKET_NAME=your-gcs-bucket-name

# AWS SES Configuration (for sending research results)
# Only if these are set, will email sending be attempted.
AWS_SES_REGION_NAME=your_aws_ses_region # e.g., us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id # Optional if running in an AWS environment with IAM roles
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key # Optional if running in an AWS environment with IAM roles
AWS_SES_SENDER_EMAIL=verified_sender@example.com # Must be a verified sender in AWS SES

